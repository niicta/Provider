<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD
  Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>

  <display-name>Provider Servlet</display-name>
  <description>
    Provider Servlet
  </description>

  <servlet>
    <servlet-name>providerServlet</servlet-name>
    <servlet-class>org.ourgroup.provider.view.jsp.ProviderServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>customerServlet</servlet-name>
    <servlet-class>org.ourgroup.provider.view.jsp.CustomerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>employeeServlet</servlet-name>
    <servlet-class>org.ourgroup.provider.view.jsp.EmployeeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>providerServlet</servlet-name>
    <url-pattern>/providerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>customerServlet</servlet-name>
    <url-pattern>/customerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>employeeServlet</servlet-name>
    <url-pattern>/employeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
